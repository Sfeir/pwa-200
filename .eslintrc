{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "globals": {
    "angular": true
  },
  "rules": {
    "curly": 0,
    "comma-dangle": 0,
    "dot-notation": 0,
    "eqeqeq": 2,
    "new-cap": 0,
    "new-parens": 0,
    "no-alert": 0,
    "no-console": 0,
    "no-constant-condition": 0,
    "no-extra-bind": 2,
    "no-extra-semi": 2,
    "no-inner-declarations": 0,
    "no-new": 0,
    "no-process-exit": 0,
    "no-proto": 0,
    "no-redeclare": 0,
    "no-return-assign": 2,
    "no-shadow": 0,
    "no-trailing-spaces": 2,
    "no-undef": 2,
    "no-unused-vars": [
      2,
      {
        "vars": "all",
        "args": "none"
      }
    ],
    "no-underscore-dangle": 0,
    "no-unused-expressions": 0,
    "no-use-before-define": 0,
    "no-extra-parens": 2,
    "quotes": [
      2,
      "single"
    ],
    "semi": 2,
    "indent": 2,
    "space-infix-ops": 2,
    "keyword-spacing": 2,
    "strict": 0,
    "valid-typeof": 2,
    "space-before-function-paren": [2, "never"],
    "space-before-blocks": [2, "always"],
    "space-in-parens": [2, "never"],
    "brace-style": [2, "1tbs", { "allowSingleLine": true }]
  }
}
